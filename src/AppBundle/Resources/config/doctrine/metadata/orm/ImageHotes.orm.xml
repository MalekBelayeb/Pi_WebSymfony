<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImageHotes" table="image_hotes">
    <id name="idImgh" type="integer" column="id_imgh">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idHote" type="integer" column="id_hote" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
