<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Question" table="question">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="question" type="string" column="Question" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reponseV" type="string" column="REPONSE_V" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reponseF" type="string" column="REPONSE_F" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reponseF2" type="string" column="REPONSE_F2" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
