<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QuestionAttempts" table="question_attempts">
    <id name="idQa" type="integer" column="id_qa">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idQuestion" type="integer" column="id_question" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attempts" type="integer" column="attempts" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
